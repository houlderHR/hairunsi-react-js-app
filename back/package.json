{
  "name": "api-hairunsi",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "build": "rm -rf ./build && tsc && cp ./src/swagger.json ./build && cp ./src/index.html ./build && cp -r ./docs ./build",
    "start": "npm run typeorm migration:run -- -d ./build/database/data-source.js;NODE_ENV=prod nodemon build/index.js",
    "dev": "docker compose down && docker compose up -d && NODE_ENV=dev npm run migration:run;NODE_ENV=dev nodemon src/index.ts",
    "test": "docker compose down && docker compose --env-file .env.test -f docker-compose.test.yml up -d && jest",
    "test:watch": "npm run test --watch",
    "test:coverage": "jest --coverage",
    "openssl:generate": "openssl req -nodes -new -x509 -keyout server.key -out server.cert",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:sync": "npm run typeorm schema:sync",
    "typeorm:cache": "npm run typeorm cache:clear",
    "schema:drop": "npm run typeorm -- -d ./src/database/data-source.ts",
    "migration:create": "npm run typeorm migration:create ./src/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm migration:generate ./src/migrations/$npm_config_name -- -d ./src/database/data-source.ts",
    "migration:show": "npm run typeorm migration:show -- -d ./src/database/data-source.ts",
    "migration:run": "npm run typeorm migration:run -- -d  ./src/database/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/database/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/nodemailer": "^6.4.14",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "googleapis": "^134.0.0",
    "http-status-codes": "^2.3.0",
    "httpolyglot": "^0.1.2",
    "json-schema-ref-parser": "^9.0.9",
    "jsonwebtoken": "^9.0.2",
    "mailtrap": "^3.3.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "nodemailer-express-handlebars": "^6.1.2",
    "nodemon": "^3.1.0",
    "pg": "^8.4.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "0.3.20",
    "typeorm-extension": "^3.5.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.11.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  }
}

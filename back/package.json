{
  "name": "api-hairunsi",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "build": "rm -rf ./build && tsc && cp ./src/swagger.json ./build && cp -r ./docs ./build",
    "prod": "typeorm-ts-node-commonjs migration:run -d ./build/database/data-source.js;NODE_ENV=prod nodemon build/index.js",
    "prod_local": "typeorm-ts-node-commonjs migration:run -d ./build/database/data-source.js;NODE_ENV=local nodemon build/index.js",
    "start": "ts-node src/index.ts",
    "dev": "typeorm-ts-node-commonjs migration:run -d ./src/database/data-source.ts;NODE_ENV=dev nodemon src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:sync": "typeorm-ts-node-commonjs schema:sync",
    "typeorm:cache": "typeorm-ts-node-commonjs cache:clear",
    "schema:drop": "typeorm-ts-node-commonjs -d ./src/database/data-source.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create ./src/migrations/$npm_config_name",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate ./src/migrations/$npm_config_name -d ./src/database/data-source.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d ./src/database/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d  ./src/database/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/database/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/nodemailer": "^6.4.14",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "googleapis": "^134.0.0",
    "http-status-codes": "^2.3.0",
    "json-schema-ref-parser": "^9.0.9",
    "jsonwebtoken": "^9.0.2",
    "mailtrap": "^3.3.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "nodemailer-express-handlebars": "^6.1.2",
    "pg": "^8.4.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "0.3.20",
    "typeorm-extension": "^3.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.11.10",
    "nodemon": "^3.1.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  }
}
